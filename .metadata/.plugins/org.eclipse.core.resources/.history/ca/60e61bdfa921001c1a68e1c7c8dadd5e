package bit;

public class Sample1 {
	public static void main(String[] args) {

//		for(int re=2 ; re<=9;re+=2) {
//			System.out.println(re+", "+(re+1));
//		}//for re

		for (int re = 2; re <= 9; re += 2) {
			for (int dan = re; dan <= 78; dan++) {
				System.out.printf("%3d",dan);
			}
		} // for re
	}
}
