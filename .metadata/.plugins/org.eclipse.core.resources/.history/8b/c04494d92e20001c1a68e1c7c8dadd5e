package variable;

public class subjectOperator {
	public static void main(String[] args) {
		// 산술 연산자
		int a = 1 + 3 ;
		int b = a - 1 ;
		int c = b * 8 ;
        int d = c / 2 ;
        int e = d % 3 ;

        
        System.out.println("a=" + a);
        System.out.println("b=" + b);
        System.out.println("c=" + c);
        System.out.println("d=" + d);
        System.out.println("e=" + e);
        /*
         
         */
	}
}
