package op;

public class 산술연산자예제 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 5;
		int b = 7;
		int c = a /b ;
		System.out.println(c);
		
		// 결과가 실수
		double d = a/b;
		//         i/i
		//          0 
		//     d = 0 
		//    0.0 
		System.out.println(d);
		
		double f = (a / b);
		//         i/i
		//          0 
		//     d = 0 
		//    0.0 
		System.out.println(f);
		
	}

}
