package 조건문;

public class IfYesNo1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char yesNo='y';
		String msg ="종료";
		if(yesNo=='y') {
			msg="계속진행";
		}
		System.out.println(msg);
		
		String msg2 = (yesNo=='y')? "계속진행":"종료";
		System.out.println(msg2);
		
	}

}
