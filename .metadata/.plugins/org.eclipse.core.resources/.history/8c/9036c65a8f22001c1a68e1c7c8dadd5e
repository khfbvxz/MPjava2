package homework;

import java.util.Scanner;

public class NanSu {

	public static void main(String[] args) {
		// 
		int num1;
		char op;
		int num2;
		
		char [] ops= {'+','-','*','/','%'};
					//0   1   2   3   4
		int inRes;
		int res;
		String msg;
		boolean isContinue;
		
		Scanner inStream = new Scanner(System.in);
		
		//난수 
		num1 = (int)(Math.random()*10);
		num2 = (int)(Math.random()*10);
		op = ops[(int)(Math.random()*5)];
		switch (op) {
		case '+':
			res = num1+num2;break;

		case '-':
			res = num1-num2;break;
		case '*':
			res = num1*num2;break;
		case '/':
			res = num1/num2;break;
		case '%':
			res = num1%num2;break;
		
		}
		
		System.out.print(num1+" "+op+" "+num2);
		inRes = inStream.nextInt();
		
		if(res==inRes) {
			msg="천재";
		}
		System.out.println(msg);
		
		
		// 정답 
		
		// 고객에게 
		
		
		
//		for (int i =1 ; i<=40; i++) {
//			int x =(int)(Math.random()*5);
//			System.out.println(x);
		
	}
}


