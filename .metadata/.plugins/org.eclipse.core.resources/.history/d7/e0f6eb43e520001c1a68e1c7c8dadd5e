package op;

public class 시프트연산자 {
	public static void main(String[] args) {
		
		byte x = 3; 
		x = x*(2^3);
		System.out.println(3 << 3); 
		System.out.println(3 << 1); 
		// 00000011(3)   ->  00011000(24)
		// 00000011(3)   ->  00000110 (6)
		System.out.println(x+21);  
		System.out.println(x+3);
		
		System.out.println(y);
	}
}
