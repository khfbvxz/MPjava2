package 반복문;

public class forEx1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 출력 형상
		System.out.print("*");
		System.out.print("*");
		System.out.print("*");
		System.out.print("*");
		System.out.print("*");
		
		for (int i=1  ;i<=6     ;i++    ) {
			System.out.print("*");
		}
	}

}
