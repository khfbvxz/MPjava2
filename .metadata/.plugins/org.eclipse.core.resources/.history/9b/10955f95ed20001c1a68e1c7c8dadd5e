package op;

import java.util.Scanner;

public class 제어문 {
	public static void main(String[] args) {
	Scanner scanner = new Scanner(System.in);
	
	
	if (x!=1) {
		System.out.print("원하는 단을 입력하세요( 다 보셨다면 1 입력하세요): ");
		
		int x = scanner.nextInt();
		for (int i=1; i<10 ; i++) {
			System.out.println(x +"*" + i+"="+x*i);
		}
	}
	else
		scanner.close();
	}
}
