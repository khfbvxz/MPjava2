package bit;

public class Exam8 {
	public static void main(String[] args) {
	
		for(int re =2 ; re<=8 ; re+=2) {//4ë²ˆ
			for(int count=1 ; count<=9;count++) {
//				for(int dan=2 ; dan<=3 ; dan++) {
				for(int dan=re ; dan<=3 ; dan++) {
					System.out.printf("%3d ==> %3d X%2d =%3d",re,dan,count,(dan*count));
				}//for dan
				System.out.println();
			}//for count
			
		}//for re
	
	}
}
