package homework;
/**
 * printf 사용
 * @author yuhwan
 *
 */
public class Exam1 {
	public static void main(String[] args) {
		//1번.
		int num1 = 3456678;
		System.out.printf("%,12d %n",num1);
		
		String s = String.format("%,12d\n", num1);
		System.out.print(num1+"==>"+s);
		String s = String.format("%12d==>%,12d\n", num1,num1);
		int a = 11;
		System.out.printf("%o",a);
				
	}
}
