package homework;
/**
 * printf 사용
 * @author yuhwan
 *
 */
public class Exam1 {
	public static void main(String[] args) {
		//1번.
		int num1 = 3456678;
		System.out.printf("%,12d",num1);
		System.out.println();
		String s = String.format("%,12d", num1);
		System.out.print(num1+"==>"+s);
		
		System.out.println();
		int a = 11;
		System.out.printf("%o",a);
				
	}
}
